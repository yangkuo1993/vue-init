.swiper-container
  height 100%
  width 100%
  .list
    display flex
    justify-content start
    .img
      width 100px
      height 100px
      overflow hidden
      img
        width 100%
    .list-info
      display flex
      flex-direction column
      justify-content space-between
      width 650px
      .title
        white-space nowrap
        text-overflow ellipsis
        overflow hidden
